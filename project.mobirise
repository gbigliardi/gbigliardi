          "_customHTML": "<section class=\"contacts4 map\" group=\"Contacts\" plugins=\"amp-main, amp-iframe\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n    <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n    <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n    <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n    <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"showContactTitle || showContactSubtitle || showContactList || showContactText || showAddress || showPhone || showEmail || showSocial\">\n    <input type=\"range\" inline title=\"Width Map\" name=\"columnWidth\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"showContactTitle || showContactSubtitle || showContactList || showContactText || showAddress || showPhone || showEmail || showSocial\">  \n    <header>Title</header>\n    <input type=\"checkbox\" title=\"Title\" name=\"showTitle\">\n    <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n    <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n        <option value=\"1\">Left</option>\n        <option value=\"2\" selected>Center</option>\n        <option value=\"3\">Right</option>\n    </select> \n    <header>Contacts</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showContactTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showContactSubtitle\">\n        <input type=\"checkbox\" title=\"List\" name=\"showContactList\">\n            <input type=\"range\" inline title=\"Count\" name=\"listCount\" min=\"1\" max=\"9\" step=\"1\" value=\"1\" condition=\"showContactList\">\n            <input type=\"range\" inline title=\"Width Marker\" name=\"widthImg\" min=\"0.8\" max=\"3\" step=\"0.1\" value=\"0.8\" condition=\"showContactList\">\n            <input type=\"checkbox\" title=\"Title List\" name=\"showContactListTitle\" condition=\"showContactList\">\n            <input type=\"checkbox\" title=\"Text List\" name=\"showContactListText\" condition=\"showContactList\" checked>\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showContactText\">\n        <input type=\"checkbox\" name=\"showAddress\" title=\"Address\" checked>\n        <input type=\"checkbox\" name=\"showPhone\" title=\"Phone\" checked>\n        <input type=\"checkbox\" name=\"showEmail\" title=\"E-mail\" checked>\n        <input type=\"checkbox\" title=\"Show Icons\" name=\"showIcons\" checked condition=\"showAddress || showPhone || showEmail\">\n        <input type=\"range\" inline title=\"Size\" name=\"iconsSize\" min=\"0.8\" max=\"6\" step=\"0.1\" value=\"1.5\" condition=\"showIcons\">\n        <input type=\"checkbox\" title=\"Show Social\" name=\"showSocial\" checked>\n            <input type=\"range\" inline title=\"Count\" name=\"socialCount\" min=\"1\" max=\"10\" value=\"2\" step=\"1\" condition=\"showSocial\">\n            <input type=\"range\" inline title=\"Size\" name=\"socialSize\" min=\"1.5\" max=\"5\" step=\"0.1\" value=\"2.7\" condition=\"showSocial\">\n        <select title=\"Align\" name=\"contactsAlign\" condition=\"showContactTitle || showContactSubtitle || showContactList || showContactText || showAddress || showPhone || showEmail || showSocial\">\n            <option value=\"1\" selected>Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n    <header>Map</header>\n        <input type=\"range\" inline title=\"Height\" name=\"mapHeight\" min=\"400\" max=\"1200\" step=\"50\" value=\"400\">\n        <input type=\"map\" title=\"Map\" name=\"googleMap\" place-id=\"ChIJs_nlB1scgEcRNQOGTL5Q5tk\" value=\"Reggio Emilia, Italia\">\n    <header>Background</header>\n        <input type=\"checkbox\" titl