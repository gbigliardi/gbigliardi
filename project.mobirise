        {
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".card-wrapper, .card-wrapper .card-box, .card-wrapper .card-img": {
              "position": "relative"
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor",
              ".mbr-overlay": {
                "background": "transparent",
                "opacity": "1"
              }
            },
            ".card-wrapper .mbr-overlay when not (@transparentBg)": {
              "background": "@cardBg",
              "opacity": "@cardOpacity"
            },
            ".card-img when (@showIcons)": {
              "display": "inline-block",
              "width": "auto",
              ".amp-iconfont": {
                "font-size": "~\"@{iconsSize}rem\"",
                "width": "~\"@{iconsSize}rem\"",
                "color": "@iconsColor !important",
                "& when ((@iconsSize * 16) > 40)": {
                  "@media (max-width: 767px)": {
                    "font-size": "40px",
                    "width": "40px"
                  }
                }
              }
            },
            ".card-wrapper when not (@transparentBg), (@showBorders), (@showCardsEffect)": {
              "@media (max-width: 991px)": {
                "padding": "15px"
              }
            },
            ".card-title when (@showLine) and (@showCardsTitle)": {
              "display": "inline-block",
              "&:after": {
                "content": "\"\"",
                "display": "block",
                "background-color": "@primaryColor",
                "height": "3px",
                "width": "25px",
                "margin": "inherit",
                "margin-top": "15px",
                "& when (@cardsAlign = '3')": {
                  "@media (max-width: 767px)": {
                    "margin-right": "auto"
                  }
                }
              }
            },
            ".card-wrapper when (@showLineEffect) and (@showLine) and (@showCardsTitle)": {
              ".card-title:after": {
                "-webkit-transition": "all .2s ease",
                "transition": "all .2s ease"
              },
              "&:hover .card-title:after": {
                "width": "100%"
              }
            },
            "& when (@showCardsEffect)": {
              ".card-wrapper > *, .card-wrapper, .amp-iconfont, .card-box > *, .card-title:after, svg": {
                "-webkit-transition": "all 0.3s linear",
                "transition": "all 0.3s linear"
              },
              ".card-wrapper:hover when not (@showBorders) and (@transparentBg)": {
                "& when (@effect = \"1\")": {
                  "-webkit-box-shadow": "0px 0px 40px 1px rgba(0, 0, 0, 0.1)",
                  "box-shadow": "0px 0px 40px 1px rgba(0, 0, 0, 0.1)"
                },
                "& when (@effect = \"2\")": {
                  "background-color": "@successColor"
                },
                ".card-img .amp-iconfont, .card-box .card-title": {
                  "color": "@primaryColor !important"
                },
                "svg": {
                  "fill": "@primaryColor !important"
                }
              },
              ".card-wrapper:hover when (@showBorders), not (@transparentBg)": {
                ".mbr-overlay": {
                  "background": "@primaryColor"
                },
                ".card-img .amp-iconfont, .card-box > *": {
                  "color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important",
                  "svg": {
                    "fill": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%)"
                  }
                },
                ".card-box .btn[class*=\"-outline\"], .card-box .btn[class*=\"-outline\"]:hover": {
                  "color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important",
                  "border-color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important"
                },
                ".card-box .card-title:after when (@showCardsTitle) and (@showLine)": {
                  "background-color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important"
                }
              }
            },
            ".mbr-row when (@showBorders) and (@showBorderTop) and not (@showPadding)": {
              "border-top": "1px solid @bordersColor"
            },
            ".mbr-row .card when not (@showPadding)": {
              "padding": "0",
              "& when (@showBorders)": {
                ".card-wrapper": {
                  "border": "none"
                },
                "@media (min-width: 768px)": {
                  "&:not(.last-child) .card-wrapper": {
                    "border-right": "1px solid @bordersColor"
                  },
                  "@media (max-width: 991px)": {
                    "&.md-pb .card-wrapper when (@cardsAmount = 4)": {
                      "border-bottom": "1px solid @bordersColor"
                    },
                    "&.md-pb:nth-child(2) .card-wrapper when (@cardsAmount = 4)": {
                      "border-right": "none"
                    }
                  }
                },
                "@media (max-width: 767px)": {
                  "&:not(.last-child) .card-wrapper": {
                    "border-right": "none",
                    "border-bottom": "1px solid @bordersColor"
                  }
                }
              }
            }
          },
          "_name": "features11",
          "_customHTML": "<section class=\"features11\" group=\"Features\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n            <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n            <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n            <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        \n        <header>Title</header>\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n            <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n            <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n                <option value=\"1\">Left</option>\n                <option value=\"2\" selected>Center</option>\n                <option value=\"3\">Right</option>\n            </select>\n        \n        <header>Cards</header>\n            <input type=\"range\" inline title=\"Count\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"3\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || checkIcons\">\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showCardsTitle\" checked>\n            <input type=\"checkbox\" title=\"Show Line\" name=\"showLine\" condition=\"showCardsTitle\" checked>\n            <input type=\"checkbox\" title=\"Show Line Effect\" name=\"showLineEffect\" condition=\"showLine && showCardsTitle\" checked>\n            <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showCardsSubtitle\">\n            <input type=\"checkbox\" title=\"Show Text\" name=\"showCardsText\" checked>\n            <input type=\"checkbox\" title=\"Show Buttons\" name=\"showCardsButtons\">\n            <select title=\"Align\" name=\"cardsAlign\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons\">\n                <option value=\"1\">Left</option>\n                <option value=\"2\" selected>Center</option>\n                <option value=\"3\">Right</option>\n            </select>\n            <input type=\"checkbox\" title=\"Show Icons\" name=\"showIcons\" checked>\n            <input type=\"range\" inline title=\"Icons Size\" name=\"iconsSize\" min=\"0.8\" max=\"4\" step=\"0.1\" value=\"2.5\" condition=\"showIcons\">\n            <input type=\"color\" title=\"Icons Color\" name=\"iconsColor\" value=\"#273272\" condition=\"showIcons\">\n            <input type=\"checkbox\" title=\"Show Cards Effect\" name=\"showCardsEffect\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons\" checked>\n                <select title=\"Effect\" name=\"effect\" condition=\"!showBorders && transparentBg && (showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons)\">\n                    <option value=\"1\" selected>Box Shadow</option>\n                    <option value=\"2\">Background</option>\n                </select>\n            <input type=\"checkbox\" title=\"Show Padding\" name=\"showPadding\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons\">\n            <input type=\"checkbox\" title=\"Borders\" name=\"showBorders\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons\" checked>\n            <input type=\"checkbox\" title=\"Borders Top\" name=\"showBorderTop\" condition=\"showBorders && !showPadding && (showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons)\">\n            <input type=\"color\" title=\"Color\" name=\"bordersColor\" value=\"#cccccc\" condition=\"showBorders && (showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons)\">\n            <input type=\"checkbox\" title=\"Transparent BG\" name=\"transparentBg\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons\" checked>\n            <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#bec1c7\" condition=\"!transparentBg && (showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons)\">\n            <input type=\"range\" inline title=\"Opacity\" name=\"cardOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"!transparentBg && (showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showIcons)\">\n        \n         <header>Background</header>\n            <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\"> \n            <input type=\"range\" inline title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"twoBgColor && bg.type == 'color'\">    \n            <fieldset type=\"background\" name=\"bg\">\n                <input type=\"image\" title=\"Background Image\" value=\"../_images/background9.jpg\">\n                <input type=\"color\" title=\"Background Color\" value=\"#f4f5f7\" selected>\n            </fieldset>\n            <input type=\"color\" title=\"2 Color\" name=\"twoColor\" value=\"#f4f5f7\" condition=\"twoBgColor && bg.type == 'color'\">  \n            <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n            <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n            <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay && bg.type !== 'color') || (twoBgColor && bg.type == 'color')\" mbr-class=\"{'mbr-col-md-4': widt